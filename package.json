{
  "name": "friday",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --no-cache --detectOpenHandles",
    "test:unit": "jest --testPathPattern='__tests__'",
    "test:hooks": "jest --testPathPattern='hooks/__tests__'",
    "test:services": "jest --testPathPattern='services/__tests__'",
    "test:utils": "jest --testPathPattern='utils/__tests__'",
    "test:components": "jest --testPathPattern='components/__tests__'",
    "test:silent": "jest --silent",
    "test:verbose": "jest --verbose"
  },
  "dependencies": {
    "@cartesia/cartesia-js": "^2.2.5",
    "@elevenlabs/elevenlabs-js": "^2.6.0",
    "@ricky0123/vad-react": "^0.0.30",
    "@ricky0123/vad-web": "^0.0.22",
    "@vercel/analytics": "^1.5.0",
    "clsx": "^2.1.1",
    "groq-sdk": "^0.19.0",
    "next": "15.4.4",
    "next-intl": "^4.3.4",
    "onnxruntime-web": "^1.21.0",
    "pusher-js": "^8.4.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.3",
    "winston": "^3.17.0",
    "zod": "^3.24.2",
    "zod-form-data": "^2.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.3",
    "typescript": "^5"
  }
}
